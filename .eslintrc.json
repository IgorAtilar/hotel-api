{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["prettier", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "linebreak-style": "off"
    }
}
